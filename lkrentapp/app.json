{
  "expo": {
    "name": "lkrental",
    "slug": "lkrental",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/lkrentlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#98ebff"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.kiaslong.lkrental"
    },
    "extra": {
      "eas": {
        "projectId": "5d06b491-426d-4f62-9ff6-ad9da43a4018"
      }
    }
    ,  "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
